using System;
using System.Linq;
using System.Collections.Generic;
public class Kata
{
    public static int[] Divisors(int n)
    {
        HashSet<int> divisors = new HashSet<int>();
        for(int i = 2; i <= Math.Sqrt(n); i++)
        {
            if(n % i == 0)
            {
                divisors.Add(i);
                divisors.Add(n / i);
            }
        }
        if(divisors.Count == 0)
            return null;
        
        int[] result = new int[divisors.Count];
        divisors.CopyTo(result);
        Array.Sort(result);
        return result;
    }
}